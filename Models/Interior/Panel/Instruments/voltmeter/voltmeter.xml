<?xml version="1.0" encoding="ISO-8859-1"?> 

<PropertyList>

  <path>voltmeter.ac</path>

  <animation>
    <!-- Objects Opaques -->
    <object-name>bezel</object-name>
    <object-name>cover</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    
    <!-- Objects Transparents -->
    <object-name>vitre</object-name>
  </animation>
  
  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/MD-312-Flamant/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needleL</object-name>
    <object-name>needleR</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>systems/electrical/outputs/volts</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>  44.45 </dep></entry>
      <entry><ind> 30 </ind><dep> -45.44 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.00229331 </x-m>
      <y-m>  0.00000000 </y-m>
      <z-m> -0.01344000 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
